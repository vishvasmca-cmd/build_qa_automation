      {
        "name": "my_leave_quick_launch",
        "type": "button",
        "primary_locator": "locator('div[class*=\"quickLaunge\"] span:has-text(\"My Leave\")')",
        "backup_locator": "locator('div[class*=\"quickLaunge\"] a:has-text(\"My Leave\")')",
