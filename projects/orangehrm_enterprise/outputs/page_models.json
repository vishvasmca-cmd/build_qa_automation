  "EmployeeListOrangehrmPage": {
    "description": "Employee List page in OrangeHRM",
    "url_pattern": "/pim/addEmployee",
    "elements": [
      {
        "name": "Employee Name Input",
        "type": "input",
        "primary_locator": "get_by_placeholder('Type for hints...')",
        "backup_locator": "locator('input[placeholder=\"Type for hints...\"]').first",
        "description": "Input field for employee name"
      },
      {
        "name": "Employee ID Input",
        "type": "input",
        "primary_locator": "locator('input[placeholder=\"Type for hints...\"]').nth(1)",
        "backup_locator": "locator('div.oxd-input-group').nth(1) input",
        "description": "Input field for employee ID"
      },
      {
        "name": "Employment Status Dropdown",
        "type": "dropdown",
        "primary_locator": "locator('div.oxd-select-text--after i')",
        "backup_locator": "locator('div.oxd-input-group').nth(2) div.oxd-select-text",
        "description": "Dropdown for selecting employment status"
      },
      {
        "name": "Include Dropdown",
        "type": "dropdown",
        "primary_locator": "locator('div.oxd-select-text--after i').nth(1)",
        "backup_locator": "locator('div.oxd-input-group').nth(3) div.oxd-select-text",
        "description": "Dropdown for selecting include options"
      },
      {
        "name": "Supervisor Name Input",
        "type": "input",
        "primary_locator": "locator('input[placeholder=\"Type for hints...\"]').nth(2)",
        "backup_locator": "locator('div.oxd-input-group').nth(4) input",
        "description": "Input field for supervisor name"
      },
      {
        "name": "Job Title Dropdown",
        "type": "dropdown",
        "primary_locator": "locator('div.oxd-select-text--after i').nth(2)",
        "backup_locator": "locator('div.oxd-input-group').nth(5) div.oxd-select-text",
        "description": "Dropdown for selecting job title"
      },
      {
        "name": "Sub Unit Dropdown",
        "type": "dropdown",
        "primary_locator": "locator('div.oxd-select-text--after i').nth(3)",
