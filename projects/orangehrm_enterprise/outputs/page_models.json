        "name": "Reset Button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Reset')",
        "backup_locator": "locator('button.oxd-button--ghost')",
        "description": "Button to reset the search filters"
      },
      {
        "name": "Search Button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Search')",
        "backup_locator": "locator('button.oxd-button--secondary')",
        "description": "Button to search employees"
      },
      {
        "name": "Add Button",
        "type": "button",
        "primary_locator": "get_by_role('button', name=' Add ')",
        "backup_locator": "locator('button.oxd-button--success')",
        "description": "Button to add a new employee"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('OrangeHRM')",
      "expect(page.locator('h6.oxd-text--h6').first).to_have_text('Employee Information')",
      "expect(page.locator('span.oxd-topbar-header-breadcrumb > h6')).to_have_text('PIM')"
    ]
  },
