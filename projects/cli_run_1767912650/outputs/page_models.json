{
  "ParabankPage": {
    "description": "Login page for customer access to ParaBank",
    "url_pattern": "/login",
    "elements": [
      {
        "name": "username_input",
        "type": "text",
        "primary_locator": "get_by_label('Username')",
        "backup_locator": "locator('input[name=\"username\"]')",
        "description": "Input field for username"
      },
      {
        "name": "password_input",
        "type": "password",
        "primary_locator": "get_by_label('Password')",
        "backup_locator": "locator('input[name=\"password\"]')",
        "description": "Input field for password"
      },
      {
        "name": "login_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Log In')",
        "backup_locator": "locator('input[value=\"Log In\"]')",
        "description": "Main submission button for login"
      },
      {
        "name": "forgot_login_info_link",
        "type": "link",
        "primary_locator": "get_by_text('Forgot login info?')",
        "backup_locator": "locator('a[href=\"lookup.htm\"]')",
        "description": "Link to recover login information"
      },
      {
        "name": "register_link",
        "type": "link",
        "primary_locator": "get_by_text('Register')",
        "backup_locator": "locator('a[href=\"register.htm\"]')",
        "description": "Link to register a new account"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('ParaBank')",
      "expect(page.get_by_text('Customer Login')).to_be_visible()",
      "expect(page.locator('input[name=\"username\"]')).to_be_visible()",
      "expect(page.locator('input[name=\"password\"]')).to_be_visible()",
      "expect(page.locator('input[value=\"Log In\"]')).to_be_visible()"
    ]
  },
  "ParabankRegistrationPage": {
    "description": "Registration page for new customers",
    "url_pattern": "/register.htm",
    "elements": [
      {
        "name": "firstName",
        "type": "textbox",
        "primary_locator": "get_by_label('First Name:')",
        "backup_locator": "locator('[name=\"customer.firstName\"]')",
        "description": "First Name input field"
      },
      {
        "name": "lastName",
        "type": "textbox",
        "primary_locator": "get_by_label('Last Name:')",
        "backup_locator": "locator('[name=\"customer.lastName\"]')",
        "description": "Last Name input field"
      },
      {
        "name": "address",
        "type": "textbox",
        "primary_locator": "get_by_label('Address:')",
        "backup_locator": "locator('[name=\"customer.address.street\"]')",
        "description": "Address input field"
      },
      {
        "name": "city",
        "type": "textbox",
        "primary_locator": "get_by_label('City:')",
        "backup_locator": "locator('[name=\"customer.address.city\"]')",
        "description": "City input field"
      },
      {
        "name": "state",
        "type": "textbox",
        "primary_locator": "get_by_label('State:')",
        "backup_locator": "locator('[name=\"customer.address.state\"]')",
        "description": "State input field"
      },
      {
        "name": "zipCode",
        "type": "textbox",
        "primary_locator": "get_by_label('Zip Code:')",
        "backup_locator": "locator('[name=\"customer.address.zipCode\"]')",
        "description": "Zip Code input field"
      },
      {
        "name": "phone",
        "type": "textbox",
        "primary_locator": "get_by_label('Phone #:')",
        "backup_locator": "locator('[name=\"customer.phoneNumber\"]')",
        "description": "Phone Number input field"
      },
      {
        "name": "ssn",
        "type": "textbox",
        "primary_locator": "get_by_label('SSN:')",
        "backup_locator": "locator('[name=\"customer.ssn\"]')",
        "description": "SSN input field"
      },
      {
        "name": "username",
        "type": "textbox",
        "primary_locator": "get_by_label('Username:')",
        "backup_locator": "locator('[name=\"customer.username\"]')",
        "description": "Username input field"
      },
      {
        "name": "password",
        "type": "textbox",
        "primary_locator": "get_by_label('Password:')",
        "backup_locator": "locator('[name=\"customer.password\"]')",
        "description": "Password input field"
      },
      {
        "name": "confirm",
        "type": "textbox",
        "primary_locator": "get_by_label('Confirm:')",
        "backup_locator": "locator('[name=\"repeatedPassword\"]')",
        "description": "Confirm Password input field"
      },
      {
        "name": "register_btn",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Register')",
        "backup_locator": "locator('input[value=\"Register\"]')",
        "description": "Register button"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('ParaBank Registration')",
      "expect(page.locator('text=Signing up is easy!')).to_be_visible()"
    ]
  }
}