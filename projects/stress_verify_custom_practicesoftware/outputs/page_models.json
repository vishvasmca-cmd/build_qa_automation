        "primary_locator": "get_by_label('Remember me')",
        "backup_locator": "locator('#rememberme')",
        "description": "Checkbox to remember the user"
      },
      {
        "name": "lost_your_password_link",
        "type": "link",
        "primary_locator": "get_by_text('Lost your password?')",
        "backup_locator": "locator('a[href*=\"lost-password\"]')",
