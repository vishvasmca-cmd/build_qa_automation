      },
      {
        "name": "Research Link",
        "type": "link",
        "primary_locator": "role=link[name='Research']",
        "backup_locator": "text=Research",
        "description": "Link to the Research page."
      },
      {
        "name": "Economic Futures Link",
        "type": "link",
        "primary_locator": "role=link[name='Economic Futures']",
        "backup_locator": "text='Economic Futures'",
        "description": "Link to the Economic Futures page."
      },
      {
        "name": "Commitments Dropdown",
        "type": "button",
        "primary_locator": "role=button[name='Commitments']",
        "backup_locator": "text=Commitments",
        "description": "Dropdown menu for Commitments."
      },
      {
        "name": "Learn Dropdown",
        "type": "button",
        "primary_locator": "role=button[name='Learn']",
        "backup_locator": "text=Learn",
        "description": "Dropdown menu for Learn."
