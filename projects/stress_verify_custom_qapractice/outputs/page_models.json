        "description": "Link to the 7 Char Val app"
      },
      {
        "name": "customer_login_link",
        "type": "link",
        "primary_locator": "get_by_role('link', name='Customer Login | Basic Shopping Cart')",
        "backup_locator": "locator('a:has-text(\"Customer Login | Basic Shopping Cart\")')",
        "description": "Link to the Customer Login | Basic Shopping Cart app"
