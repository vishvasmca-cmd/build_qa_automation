  "AlertsJavascriptPage": {
    "description": "Page demonstrating JavaScript alerts, confirms, and prompts.",
    "url_pattern": "https://testpages.herokuapp.com/pages/basics/alerts-javascript/",
    "elements": [
      {
        "name": "show_alert_box_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Show alert box')",
        "backup_locator": "locator('#alertexamples')",
        "locator_strategy": "primary.or_(backup)",
        "description": "Button to trigger a JavaScript alert."
      },
      {
        "name": "show_confirm_box_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Show confirm box')",
        "backup_locator": "locator('#confirmexample')",
        "locator_strategy": "primary.or_(backup)",
        "description": "Button to trigger a JavaScript confirm."
      },
      {
        "name": "show_prompt_box_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Show prompt box')",
        "backup_locator": "locator('#promptexample')",
        "locator_strategy": "primary.or_(backup)",
        "description": "Button to trigger a JavaScript prompt."
      }
    ],
    "assertions": [
      "expect(page).to_have_title('Alerts - JavaScript')",
      "expect(page.get_by_role('heading', name='Alerts - JavaScript')).to_be_visible()"
    ]
  },
