{
  "HomePage": {
    "description": "This is the login page for the Swag Labs demo application.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input[id='user-name']",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input[id='password']",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input[id='login-button']",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SwagLabsLoginPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory page.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input#user-name",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input#password",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input#login-button",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames",
        "type": "text",
        "primary_locator": "//div[contains(text(),'Accepted usernames are:')]",
        "backup_locator": "div:nth-child(3)",
        "description": "Text displaying accepted usernames"
      },
      {
        "name": "Password for all users",
        "type": "text",
        "primary_locator": "//div[contains(text(),'Password for all users:')]",
        "backup_locator": "div:nth-child(4)",
        "description": "Text displaying password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "UnknownPage": {
    "description": "This page displays a list of available products with their names, descriptions, prices, and 'Add to cart' buttons.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Name",
        "type": "text",
        "primary_locator": "text=Sauce Labs Backpack",
        "backup_locator": "css=.inventory_item_name",
        "description": "Name of the product."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": "text=carry.allTheThings() with the sleek",
        "backup_locator": "css=.inventory_item_desc",
        "description": "Description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": "text=$29.99",
        "backup_locator": "css=.inventory_item_price",
        "description": "Price of the product."
      },
      {
        "name": "Add to cart",
        "type": "button",
        "primary_locator": "text=Add to cart",
        "backup_locator": "css=.btn_primary",
        "description": "Button to add the product to the cart."
      },
      {
        "name": "Products Header",
        "type": "text",
        "primary_locator": "text=Products",
        "backup_locator": "css=.title",
        "description": "The 'Products' header at the top of the page."
      },
      {
        "name": "Filter Button",
        "type": "button",
        "primary_locator": "css=[data-test='product_sort_container']",
        "backup_locator": "text=Name (A to Z)",
        "description": "Button to filter the products."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "The 'Products' header is displayed",
      "At least one product is displayed with its name, description, and price",
      "The 'Add to cart' button is displayed for each product"
    ]
  }
}