{
  "HomePage": {
    "description": "This is the login page for the Swag Labs e-commerce website. Users can enter their username and password to access the product catalog.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input#user-name",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input#password",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input#login-button",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames are:",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "div.login_credentials_wrap > div > div.login_credentials",
        "description": "Text displaying accepted usernames"
      },
      {
        "name": "Password for all users:",
        "type": "text",
        "primary_locator": "text=Password for all users:",
        "backup_locator": "div.login_password",
        "description": "Text displaying password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SwagLabsLoginPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory page.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "role=textbox[name=\"Username\"]",
        "backup_locator": "css=input[id=\"user-name\"]",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "role=textbox[name=\"Password\"]",
        "backup_locator": "css=input[id=\"password\"]",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "role=button[name=\"Login\"]",
        "backup_locator": "css=input[id=\"login-button\"]",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "UnknownPage": {
    "description": "This is the inventory page of the Swag Labs demo website, displaying a list of available products.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Sort Dropdown",
        "type": "dropdown",
        "primary_locator": "data-test=\"product_sort_container\"",
        "backup_locator": "#header_container > div.header_secondary_container > div.right_component > span > select",
        "description": "Dropdown to sort the products by name or price."
      },
      {
        "name": "Add to cart button for Sauce Labs Backpack",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-sauce-labs-backpack\"",
        "backup_locator": "div.inventory_item:nth-child(1) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Sauce Labs Backpack to the shopping cart."
      },
      {
        "name": "Add to cart button for Sauce Labs Bike Light",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-sauce-labs-bike-light\"",
        "backup_locator": "div.inventory_item:nth-child(2) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Sauce Labs Bike Light to the shopping cart."
      },
      {
        "name": "Add to cart button for Sauce Labs Bolt T-Shirt",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-sauce-labs-bolt-t-shirt\"",
        "backup_locator": "div.inventory_item:nth-child(3) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Sauce Labs Bolt T-Shirt to the shopping cart."
      },
      {
        "name": "Add to cart button for Sauce Labs Fleece Jacket",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-sauce-labs-fleece-jacket\"",
        "backup_locator": "div.inventory_item:nth-child(4) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Sauce Labs Fleece Jacket to the shopping cart."
      },
      {
        "name": "Add to cart button for Sauce Labs Onesie",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-sauce-labs-onesie\"",
        "backup_locator": "div.inventory_item:nth-child(5) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Sauce Labs Onesie to the shopping cart."
      },
      {
        "name": "Add to cart button for Test.allTheThings() T-Shirt (Red)",
        "type": "button",
        "primary_locator": "data-test=\"add-to-cart-test.allthethings()-t-shirt-(red)\"",
        "backup_locator": "div.inventory_item:nth-child(6) > div:nth-child(2) > div:nth-child(2) > button",
        "description": "Button to add the Test.allTheThings() T-Shirt (Red) to the shopping cart."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Page contains the 'Products' header",
      "At least one product item is displayed"
    ]
  }
}