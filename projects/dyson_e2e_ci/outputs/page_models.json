{
  "HomePage": {
    "description": "This is the homepage for Dyson India. The primary purpose is to showcase Dyson products and provide navigation to different product categories and information.",
    "url_pattern": "https://www.dyson.in/",
    "elements": [
      {
        "name": "Dyson Logo",
        "type": "link",
        "primary_locator": "//a[@aria-label='Dyson']",
        "backup_locator": "a[href='/']",
        "description": "Link to the Dyson homepage."
      },
      {
        "name": "Search Products and Parts",
        "type": "search_box",
        "primary_locator": "xpath=//input[@placeholder='Search products and parts']",
        "backup_locator": "css=input[placeholder='Search products and parts']",
        "description": "Search input field for products and parts."
      },
      {
        "name": "Shop Now",
        "type": "button",
        "primary_locator": "text=Shop now",
        "backup_locator": "css=a[aria-label='Shop now']",
        "description": "Button to shop for the featured product."
      },
      {
        "name": "Vacuum & wet cleaners",
        "type": "link",
        "primary_locator": "text=Vacuum & wet cleaners",
        "backup_locator": "css=a[href='/vacuum-cleaners']",
        "description": "Link to the vacuum cleaners category."
      },
      {
        "name": "Air purifier",
        "type": "link",
        "primary_locator": "text=Air purifier",
        "backup_locator": "css=a[href='/air-treatment']",
        "description": "Link to the air purifier category."
      },
      {
        "name": "Headphones",
        "type": "link",
        "primary_locator": "text=Headphones",
        "backup_locator": "css=a[href='/headphones']",
        "description": "Link to the headphones category."
      },
      {
        "name": "Lighting",
        "type": "link",
        "primary_locator": "text=Lighting",
        "backup_locator": "css=a[href='/lighting']",
        "description": "Link to the lighting category."
      },
      {
        "name": "Support",
        "type": "link",
        "primary_locator": "text=Support",
        "backup_locator": "css=a[href='/support']",
        "description": "Link to the support page."
      },
      {
        "name": "Best sellers",
        "type": "link",
        "primary_locator": "text=Best sellers",
        "backup_locator": "css=a[href='/best-sellers']",
        "description": "Link to the best sellers page."
      }
    ],
    "assertions": [
      "Page title contains 'Dyson'",
      "The Dyson logo is visible",
      "The search input field is present",
      "The 'Shop now' button is visible",
      "The 'Vacuum & wet cleaners' link is present"
    ]
  }
}