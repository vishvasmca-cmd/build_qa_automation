{
  "HomePage": {
    "description": "The Dyson India homepage showcasing products and promotions.",
    "url_pattern": "https://www.dyson.in/",
    "elements": [
      {
        "name": "Dyson Logo",
        "type": "image",
        "primary_locator": "//a[@aria-label='Homepage']",
        "backup_locator": "css=svg[class*='DysonLogo']",
        "description": "Dyson logo in the top left corner."
      },
      {
        "name": "Search Products and Parts",
        "type": "search box",
        "primary_locator": "css=input[placeholder='Search products and parts']",
        "backup_locator": "css=div[class*='search']",
        "description": "Search input box to find products and parts."
      },
      {
        "name": "Shop Now",
        "type": "button",
        "primary_locator": "text=Shop now",
        "backup_locator": "css=a[class*='Button']",
        "description": "Button to navigate to the product page."
      },
      {
        "name": "Vacuum & wet cleaners",
        "type": "link",
        "primary_locator": "text=Vacuum & wet cleaners",
        "backup_locator": "css=a[href='/vacuum-cleaners']",
        "description": "Link to the vacuum cleaners category."
      },
      {
        "name": "Air purifier",
        "type": "link",
        "primary_locator": "text=Air purifier",
        "backup_locator": "css=a[href='/air-treatment']",
        "description": "Link to the air purifier category."
      },
      {
        "name": "Headphones",
        "type": "link",
        "primary_locator": "text=Headphones",
        "backup_locator": "css=a[href='/headphones']",
        "description": "Link to the headphones category."
      },
      {
        "name": "Lighting",
        "type": "link",
        "primary_locator": "text=Lighting",
        "backup_locator": "css=a[href='/lighting']",
        "description": "Link to the lighting category."
      },
      {
        "name": "Support",
        "type": "link",
        "primary_locator": "text=Support",
        "backup_locator": "css=a[href='/support']",
        "description": "Link to the support page."
      },
      {
        "name": "Best sellers",
        "type": "link",
        "primary_locator": "text=Best sellers",
        "backup_locator": "css=a[href='/best-sellers']",
        "description": "Link to the best sellers page."
      }
    ],
    "assertions": [
      "Title contains 'Dyson'",
      "Page contains the text 'Compact. Powerful. Yet quiet.'",
      "Page contains the text 'Dyson.in exclusive: 24 months no cost EMI'"
    ]
  }
}