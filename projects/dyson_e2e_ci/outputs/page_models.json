{
  "HomePage": {
    "description": "This is the homepage for Dyson India. The primary purpose is to showcase Dyson products, particularly the Hushjet Purifier Compact, and provide navigation to different product categories and information.",
    "url_pattern": "https://www.dyson.in/",
    "elements": [
      {
        "name": "Dyson Logo",
        "type": "link",
        "primary_locator": "//a[@aria-label='Dyson']",
        "backup_locator": "img[alt='Dyson']",
        "description": "Link to the Dyson homepage."
      },
      {
        "name": "Search Products and Parts",
        "type": "search_box",
        "primary_locator": "css=button[aria-label='Search products and parts']",
        "backup_locator": "text=Search products and parts",
        "description": "Search input field to find specific products or parts."
      },
      {
        "name": "Shop Now (Hushjet)",
        "type": "button",
        "primary_locator": "text=Shop now",
        "backup_locator": "css=.button--link",
        "description": "Button to navigate to the Hushjet Purifier Compact product page."
      },
      {
        "name": "Deals",
        "type": "link",
        "primary_locator": "text=Deals",
        "backup_locator": "a[href='/deals.html']",
        "description": "Link to the deals page."
      },
      {
        "name": "Vacuum & wet cleaners",
        "type": "link",
        "primary_locator": "text=Vacuum & wet cleaners",
        "backup_locator": "a[href='/vacuum-cleaners.html']",
        "description": "Link to the vacuum cleaners page."
      },
      {
        "name": "Air purifier",
        "type": "link",
        "primary_locator": "text=Air purifier",
        "backup_locator": "a[href='/air-treatment.html']",
        "description": "Link to the air purifier page."
      },
      {
        "name": "Headphones",
        "type": "link",
        "primary_locator": "text=Headphones",
        "backup_locator": "a[href='/headphones.html']",
        "description": "Link to the headphones page."
      },
      {
        "name": "Lighting",
        "type": "link",
        "primary_locator": "text=Lighting",
        "backup_locator": "a[href='/lighting.html']",
        "description": "Link to the lighting page."
      },
      {
        "name": "Support",
        "type": "link",
        "primary_locator": "text=Support",
        "backup_locator": "a[href='/support.html']",
        "description": "Link to the support page."
      },
      {
        "name": "Best sellers",
        "type": "link",
        "primary_locator": "text=Best sellers",
        "backup_locator": "a[href='/best-sellers.html']",
        "description": "Link to the best sellers page."
      }
    ],
    "assertions": [
      "Page title contains 'Dyson'",
      "The 'Dyson Logo' is visible",
      "The 'Search products and parts' search box is visible",
      "The 'Shop now' button is visible",
      "The text 'Compact. Powerful. Yet quiet.' is visible"
    ]
  }
}