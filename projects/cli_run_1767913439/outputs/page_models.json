{
  "ParabankHomePage": {
    "description": "Home page for ParaBank with customer login and links to services",
    "url_pattern": "/parabank/index.htm",
    "elements": [
      {
        "name": "username_field",
        "type": "textbox",
        "primary_locator": "get_by_label('Username')",
        "backup_locator": "locator('input[name=\"username\"]')",
        "description": "Input field for the user's username"
      },
      {
        "name": "password_field",
        "type": "textbox",
        "primary_locator": "get_by_label('Password')",
        "backup_locator": "locator('input[name=\"password\"]')",
        "description": "Input field for the user's password"
      },
      {
        "name": "login_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Log In')",
        "backup_locator": "locator('input[value=\"Log In\"]')",
        "description": "Button to submit the login form"
      },
      {
        "name": "register_link",
        "type": "link",
        "primary_locator": "get_by_role('link', name='Register')",
        "backup_locator": "locator('a[href=\"register.htm\"]')",
        "description": "Link to the registration page"
      },
      {
        "name": "forgot_login_info_link",
        "type": "link",
        "primary_locator": "get_by_role('link', name='Forgot login info?')",
        "backup_locator": "locator('a[href=\"lookupcustomer.htm\"]')",
        "description": "Link to the forgot login info page"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('ParaBank | Welcome')",
      "expect(page.locator('//h2[text()=\"Customer Login\"]')).to_be_visible()",
      "expect(page.locator('//h2[text()=\"Customer Login\"]')).to_contain_text('Customer Login')"
    ]
  },
  "ParabankRegisterPage": {
    "description": "Registration page for new customer accounts",
    "url_pattern": "/register",
    "elements": [
      {
        "name": "first_name",
        "type": "textbox",
        "primary_locator": "get_by_label('First Name:')",
        "backup_locator": "locator('input[name=\"customer.firstName\"]')",
        "description": "Input field for the first name of the user"
      },
      {
        "name": "last_name",
        "type": "textbox",
        "primary_locator": "get_by_label('Last Name:')",
        "backup_locator": "locator('input[name=\"customer.lastName\"]')",
        "description": "Input field for the last name of the user"
      },
      {
        "name": "address",
        "type": "textbox",
        "primary_locator": "get_by_label('Address:')",
        "backup_locator": "locator('input[name=\"customer.address.street\"]')",
        "description": "Input field for the address of the user"
      },
      {
        "name": "city",
        "type": "textbox",
        "primary_locator": "get_by_label('City:')",
        "backup_locator": "locator('input[name=\"customer.address.city\"]')",
        "description": "Input field for the city of the user"
      },
      {
        "name": "state",
        "type": "textbox",
        "primary_locator": "get_by_label('State:')",
        "backup_locator": "locator('input[name=\"customer.address.state\"]')",
        "description": "Input field for the state of the user"
      },
      {
        "name": "zip_code",
        "type": "textbox",
        "primary_locator": "get_by_label('Zip Code:')",
        "backup_locator": "locator('input[name=\"customer.address.zipCode\"]')",
        "description": "Input field for the zip code of the user"
      },
      {
        "name": "phone",
        "type": "textbox",
        "primary_locator": "get_by_label('Phone #:')",
        "backup_locator": "locator('input[name=\"customer.phoneNumber\"]')",
        "description": "Input field for the phone number of the user"
      },
      {
        "name": "ssn",
        "type": "textbox",
        "primary_locator": "get_by_label('SSN:')",
        "backup_locator": "locator('input[name=\"customer.ssn\"]')",
        "description": "Input field for the ssn of the user"
      },
      {
        "name": "username",
        "type": "textbox",
        "primary_locator": "get_by_label('Username:')",
        "backup_locator": "locator('input[name=\"customer.username\"]')",
        "description": "Input field for the username of the user"
      },
      {
        "name": "password",
        "type": "password",
        "primary_locator": "get_by_label('Password:')",
        "backup_locator": "locator('input[name=\"customer.password\"]')",
        "description": "Input field for the password of the user"
      },
      {
        "name": "confirm",
        "type": "password",
        "primary_locator": "get_by_label('Confirm:')",
        "backup_locator": "locator('input[name=\"repeatedPassword\"]')",
        "description": "Input field to confirm the password of the user"
      },
      {
        "name": "register_btn",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Register')",
        "backup_locator": "locator('input[value=\"Register\"]')",
        "description": "Submission button for registration"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('ParaBank Registration')",
      "expect(page.get_by_text('Signing up is easy!')).to_be_visible()"
    ]
  }
}