{
  "HomePage": {
    "description": "This is the home page of the Swag Labs e-commerce website, displaying a list of available products.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Product Name",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/a/div[@class='inventory_item_name']",
        "backup_locator": ".inventory_item_name",
        "description": "The name of the product."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/div[@class='inventory_item_desc']",
        "backup_locator": ".inventory_item_desc",
        "description": "The description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/div[@class='inventory_item_price']",
        "backup_locator": ".inventory_item_price",
        "description": "The price of the product."
      },
      {
        "name": "Add to Cart Button",
        "type": "button",
        "primary_locator": "//button[contains(text(),'Add to cart')]",
        "backup_locator": "button:contains('Add to cart')",
        "description": "Button to add the product to the shopping cart."
      },
      {
        "name": "Shopping Cart Icon",
        "type": "link",
        "primary_locator": "[data-icon='shopping-cart']",
        "backup_locator": ".shopping_cart_link",
        "description": "Link to the shopping cart page."
      },
      {
        "name": "Product Sort Container",
        "type": "dropdown",
        "primary_locator": "[data-test='product_sort_container']",
        "backup_locator": ".product_sort_container",
        "description": "Dropdown to sort the products."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "At least one product is displayed",
      "The 'Add to cart' button is visible for each product"
    ]
  },
  "SwagLabsPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory page.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "id=user-name",
        "backup_locator": "css=input[placeholder='Username']",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "id=password",
        "backup_locator": "css=input[placeholder='Password']",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "id=login-button",
        "backup_locator": "css=input[value='Login']",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "css=.login_credentials_wrap div:nth-child(1)",
        "description": "Text indicating the accepted usernames"
      },
      {
        "name": "Password for all users",
        "type": "text",
        "primary_locator": "text=Password for all users:",
        "backup_locator": "css=.login_credentials_wrap div:nth-child(2)",
        "description": "Text indicating the password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present",
      "The text 'Accepted usernames are:' is present",
      "The text 'Password for all users:' is present"
    ]
  },
  "SaucedemoInventoryPage": {
    "description": "The inventory page displays a list of available products with their names, descriptions, prices, and 'Add to cart' buttons.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Name",
        "type": "text",
        "primary_locator": "[data-test*='title']",
        "backup_locator": ".inventory_item_name",
        "description": "The name of the product."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": ".inventory_item_desc",
        "backup_locator": ".inventory_item_label > div:nth-child(2)",
        "description": "The description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": ".inventory_item_price",
        "backup_locator": ".inventory_item_label > div.inventory_item_price",
        "description": "The price of the product."
      },
      {
        "name": "Add to Cart Button",
        "type": "button",
        "primary_locator": "[data-test^='add-to-cart']",
        "backup_locator": "button:contains('Add to cart')",
        "description": "Button to add the product to the shopping cart."
      },
      {
        "name": "Product Image",
        "type": "image",
        "primary_locator": ".inventory_item_img img",
        "backup_locator": ".inventory_item_img a > img",
        "description": "Image of the product."
      },
      {
        "name": "Products Header",
        "type": "text",
        "primary_locator": "Products",
        "backup_locator": "div.header_secondary_container > span",
        "description": "The 'Products' header text."
      },
      {
        "name": "Filter Products",
        "type": "dropdown",
        "primary_locator": "[data-test='product_sort_container']",
        "backup_locator": "span.select_container",
        "description": "Dropdown to filter products by name or price."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "The 'Products' header is displayed",
      "At least one product is displayed with a name, description, price, and 'Add to cart' button"
    ]
  },
  "UnknownPage": {
    "description": "This is the login page for the Swag Labs application, where users can enter their credentials to access the application's features.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "id=user-name",
        "backup_locator": "css=input[placeholder='Username']",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "id=password",
        "backup_locator": "css=input[placeholder='Password']",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "id=login-button",
        "backup_locator": "css=input[value='Login']",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "css=.login_credentials",
        "description": "Text displaying accepted usernames"
