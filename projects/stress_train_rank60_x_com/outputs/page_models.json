{
  "UnknownPage": {
    "description": "Landing/Signup page for X (formerly Twitter)",
    "url_pattern": "https://x.com/",
    "elements": [
      {
        "name": "sign_up_with_google_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Sign up with Google')",
        "backup_locator": "locator('div:has-text(\"Sign up with Google\")')",
        "description": "Button to sign up using Google account"
      },
      {
        "name": "sign_up_with_apple_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Sign up with Apple')",
        "backup_locator": "locator('div:has-text(\"Sign up with Apple\")')",
        "description": "Button to sign up using Apple account"
      },
      {
        "name": "create_account_button",
        "type": "button",
        "primary_locator": "get_by_role('button', name='Create account')",
        "backup_locator": "locator('div:has-text(\"Create account\")')",
        "description": "Button to create a new account"
      },
      {
        "name": "sign_in_link",
        "type": "link",
        "primary_locator": "get_by_role('link', name='Sign in')",
        "backup_locator": "locator('div:has-text(\"Sign in\")')",
        "description": "Link to sign into an existing account"
      },
      {
        "name": "get_grok_link",
        "type": "link",
        "primary_locator": "get_by_role('link', name='Get Grok')",
        "backup_locator": "locator('div:has-text(\"Get Grok\")')",
        "description": "Link to Get Grok"
      }
    ],
    "assertions": [
      "expect(page).to_have_title('X')",
      "expect(page.locator('h1:has-text(\"Happening now\")')).to_be_visible()",
      "expect(page.locator('h2:has-text(\"Join today.\")')).to_be_visible()"
    ]
  }
}