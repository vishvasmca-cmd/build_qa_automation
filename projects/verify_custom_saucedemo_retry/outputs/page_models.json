{
  "HomePage": {
    "description": "This is the login page for the Swag Labs demo application.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input[id='user-name']",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input[id='password']",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input[id='login-button']",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SwagLabsLoginPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory page.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input#user-name",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input#password",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input#login-button",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames",
        "type": "text",
        "primary_locator": "//div[contains(text(),'Accepted usernames are:')]",
        "backup_locator": "div:nth-child(3)",
        "description": "Text displaying accepted usernames"
      },
      {
        "name": "Password for all users",
        "type": "text",
        "primary_locator": "//div[contains(text(),'Password for all users:')]",
        "backup_locator": "div:nth-child(4)",
        "description": "Text displaying password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SaucedemoInventoryPage": {
    "description": "The Saucedemo Inventory Page displays a list of available products with their descriptions, prices, and 'Add to cart' or 'Remove' buttons.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Name",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/a/div[@class='inventory_item_name']",
        "backup_locator": ".inventory_item_name",
        "description": "The name of the product."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/div[@class='inventory_item_desc']",
        "backup_locator": ".inventory_item_desc",
        "description": "The description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/div[@class='inventory_item_price']",
        "backup_locator": ".inventory_item_price",
        "description": "The price of the product."
      },
      {
        "name": "Add to cart button",
        "type": "button",
        "primary_locator": "//button[contains(@id, 'add-to-cart')]",
        "backup_locator": "button:contains('Add to cart')",
        "description": "Button to add the product to the shopping cart."
      },
      {
        "name": "Remove button",
        "type": "button",
        "primary_locator": "//button[contains(@id, 'remove')]",
        "backup_locator": "button:contains('Remove')",
        "description": "Button to remove the product from the shopping cart."
      },
      {
        "name": "Products Header",
        "type": "text",
        "primary_locator": "//span[@class='title']",
        "backup_locator": "span.title",
        "description": "The 'Products' header text."
      },
      {
        "name": "Filter Dropdown",
        "type": "dropdown",
        "primary_locator": "[data-test='product_sort_container']",
        "backup_locator": ".product_sort_container",
        "description": "Dropdown to filter products."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "The 'Products' header is displayed",
      "At least one product is displayed with its name, description, and price",
      "The 'Add to cart' or 'Remove' button is displayed for each product"
    ]
  }
}