{
  "HomePage": {
    "description": "This is the login page for the Swag Labs e-commerce website. Users can enter their username and password to access the product catalog.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input#user-name",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input#password",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input#login-button",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames are:",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "div.login_credentials_wrap > div > div.login_credentials",
        "description": "Text displaying accepted usernames"
      },
      {
        "name": "Password for all users:",
        "type": "text",
        "primary_locator": "text=Password for all users:",
        "backup_locator": "div.login_password",
        "description": "Text displaying password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SwagLabsLoginPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory page.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "role=textbox[name=\"Username\"]",
        "backup_locator": "css=input[id=\"user-name\"]",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "role=textbox[name=\"Password\"]",
        "backup_locator": "css=input[id=\"password\"]",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "role=button[name=\"Login\"]",
        "backup_locator": "css=input[id=\"login-button\"]",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SaucedemoInventoryPage": {
    "description": "This page displays the inventory of products available for purchase on the Saucedemo website.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Name",
        "type": "link",
        "primary_locator": "//div[@class='inventory_item_label']/a/div[@class='inventory_item_name ']",
        "backup_locator": ".inventory_item_name",
        "description": "The name of the product, which is a link to the product details page."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_label']/div[@class='inventory_item_desc']",
        "backup_locator": ".inventory_item_desc",
        "description": "The description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_price']",
        "backup_locator": ".inventory_item_price",
        "description": "The price of the product."
      },
      {
        "name": "Add to Cart Button",
        "type": "button",
        "primary_locator": "//button[contains(@id, 'add-to-cart')]",
        "backup_locator": "button[class='btn btn_primary btn_small btn_inventory']",
        "description": "Button to add the product to the shopping cart."
      },
      {
        "name": "Remove Button",
        "type": "button",
        "primary_locator": "//button[contains(@id, 'remove')]",
        "backup_locator": "button[class='btn btn_secondary btn_small btn_inventory']",
        "description": "Button to remove the product from the shopping cart."
      },
      {
        "name": "Products Header",
        "type": "text",
        "primary_locator": "//span[text()='Products']",
        "backup_locator": ".title",
        "description": "The header text indicating the page displays products."
      },
      {
        "name": "Filter/Sort Dropdown",
        "type": "dropdown",
        "primary_locator": "//select[@class='product_sort_container']",
        "backup_locator": ".product_sort_container",
        "description": "Dropdown to filter or sort the products."
      },
      {
        "name": "Shopping Cart Link",
        "type": "link",
        "primary_locator": "//a[@class='shopping_cart_link']",
        "backup_locator": ".shopping_cart_link",
        "description": "Link to the shopping cart page."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "The 'Products' header is displayed",
      "At least one product is displayed on the page",
      "The 'Add to Cart' button is displayed for each product",
      "The filter/sort dropdown is displayed"
    ]
  }
}