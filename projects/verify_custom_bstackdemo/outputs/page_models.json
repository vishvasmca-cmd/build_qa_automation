{
  "HomePage": {
    "description": "The home page of the BrowserStack demo website, showcasing available products and allowing users to add them to the cart.",
    "url_pattern": "https://bstackdemo.com/",
    "elements": [
      {
        "name": "Apple Filter",
        "type": "button",
        "primary_locator": "data-testid=Apple",
        "backup_locator": "text=Apple",
        "description": "Filter products by Apple vendor."
      },
      {
        "name": "Samsung Filter",
        "type": "button",
        "primary_locator": "data-testid=Samsung",
        "backup_locator": "text=Samsung",
        "description": "Filter products by Samsung vendor."
      },
      {
        "name": "Google Filter",
        "type": "button",
        "primary_locator": "data-testid=Google",
        "backup_locator": "text=Google",
        "description": "Filter products by Google vendor."
      },
      {
        "name": "OnePlus Filter",
        "type": "button",
        "primary_locator": "data-testid=OnePlus",
        "backup_locator": "text=OnePlus",
        "description": "Filter products by OnePlus vendor."
      },
      {
        "name": "Add to cart button for iPhone 12",
        "type": "button",
        "primary_locator": "data-testid=Add to cart",
        "backup_locator": "//div[contains(text(),'iPhone 12')]/following-sibling::button",
        "description": "Button to add the iPhone 12 to the shopping cart."
      },
      {
        "name": "Add to cart button for iPhone 12 Mini",
        "type": "button",
        "primary_locator": "data-testid=Add to cart",
        "backup_locator": "//div[contains(text(),'iPhone 12 Mini')]/following-sibling::button",
        "description": "Button to add the iPhone 12 Mini to the shopping cart."
      },
      {
        "name": "Bag Icon",
        "type": "button",
        "primary_locator": "data-testid=Bag",
        "backup_locator": "css=svg[aria-label='Bag']",
        "description": "Icon to open the shopping cart."
      },
      {
        "name": "Checkout Button",
        "type": "button",
        "primary_locator": "data-testid=Checkout",
        "backup_locator": "text=CHECKOUT",
        "description": "Button to navigate to the checkout page."
      },
      {
        "name": "Search Input",
        "type": "input",
        "primary_locator": "id=search",
        "backup_locator": "css=input[type='text']",
        "description": "Input field for searching products."
      },
      {
        "name": "Search Button",
        "type": "button",
        "primary_locator": "text=Search",
        "backup_locator": "css=button[aria-label='Search']",
        "description": "Button to initiate the search."
      },
      {
        "name": "Sign In Link",
        "type": "link",
        "primary_locator": "text=Sign In",
        "backup_locator": "css=a[href='/signin']",
        "description": "Link to navigate to the sign-in page."
      }
    ],
    "assertions": [
      "Page title contains 'BrowserStack Demo'",
      "Page contains the text 'Shop smarter with our mobile e-commerce platform!'",
      "At least one product is displayed on the page",
      "The 'Bag' icon is visible"
    ]
  },
  "StackdemoPage": {
    "description": "This page is the sign-in page for the BrowserStack demo application. It allows users to select a username and password from dropdown menus and log in.",
    "url_pattern": "https://bstackdemo.com/signin?checkout=true",
    "elements": [
      {
        "name": "Username Dropdown",
        "type": "dropdown",
        "primary_locator": "id=username",
        "backup_locator": "css=select[name='username']",
        "description": "Dropdown menu to select a username."
      },
      {
        "name": "Password Dropdown",
        "type": "dropdown",
        "primary_locator": "id=password",
        "backup_locator": "css=select[name='password']",
        "description": "Dropdown menu to select a password."
      },
      {
        "name": "Log In Button",
        "type": "button",
        "primary_locator": "id=login-btn",
        "backup_locator": "css=#login-btn",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title contains 'BrowserStack Demo'",
      "The 'Select Username' dropdown is present",
      "The 'Select Password' dropdown is present",
      "The 'Log In' button is present"
    ]
  },
  "BrowserstackDemoLoginPage": {
    "description": "This is the login page for the BrowserStack demo application. It allows users to select a username and password from dropdown menus and log in.",
    "url_pattern": "https://bstackdemo.com/signin*",
    "elements": [
      {
        "name": "Username Dropdown",
        "type": "dropdown",
        "primary_locator": "id=username",
        "backup_locator": "css=#username",
        "description": "Dropdown to select a username."
      },
      {
        "name": "Password Dropdown",
        "type": "dropdown",
        "primary_locator": "id=password",
        "backup_locator": "css=#password",
        "description": "Dropdown to select a password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "id=login-btn",
        "backup_locator": "text=Log In",
        "description": "Button to submit the login form."
      }
    ],
    "assertions": [
      "Page title contains 'BrowserStack Demo'",
      "Logo is visible",
      "Username dropdown is present",
      "Password dropdown is present",
      "Login button is present"
    ]
  }
}