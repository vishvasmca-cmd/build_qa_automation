{
  "HomePage": {
    "description": "This is the login page for the Swag Labs e-commerce website. Users can enter their username and password to access the product catalog.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "[data-test='username']",
        "backup_locator": "input#user-name",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "[data-test='password']",
        "backup_locator": "input#password",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "[data-test='login-button']",
        "backup_locator": "input#login-button",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames are:",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "div.login_credentials_wrap > div > div.login_credentials",
        "description": "Text displaying accepted usernames"
      },
      {
        "name": "Password for all users:",
        "type": "text",
        "primary_locator": "text=Password for all users:",
        "backup_locator": "div.login_password",
        "description": "Text displaying password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SwagLabsPage": {
    "description": "This is the login page for Swag Labs, where users can enter their credentials to access the inventory.",
    "url_pattern": "https://www.saucedemo.com/",
    "elements": [
      {
        "name": "Username Input",
        "type": "text field",
        "primary_locator": "id=user-name",
        "backup_locator": "css=input[placeholder='Username']",
        "description": "Input field for the username."
      },
      {
        "name": "Password Input",
        "type": "text field",
        "primary_locator": "id=password",
        "backup_locator": "css=input[placeholder='Password']",
        "description": "Input field for the password."
      },
      {
        "name": "Login Button",
        "type": "button",
        "primary_locator": "id=login-button",
        "backup_locator": "css=input[value='Login']",
        "description": "Button to submit the login form."
      },
      {
        "name": "Accepted usernames are:",
        "type": "text",
        "primary_locator": "text=Accepted usernames are:",
        "backup_locator": "css=div.login_credentials_wrap > div.login_credentials",
        "description": "Text indicating accepted usernames"
      },
      {
        "name": "Password for all users:",
        "type": "text",
        "primary_locator": "text=Password for all users:",
        "backup_locator": "css=div.login_password",
        "description": "Text indicating password for all users"
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Login button is present",
      "Username input field is present",
      "Password input field is present"
    ]
  },
  "SaucedemoInventoryPage": {
    "description": "The inventory page displays a list of available products with their names, descriptions, prices, and 'Add to cart' buttons. It also includes a sorting dropdown to change the order of the products.",
    "url_pattern": "https://www.saucedemo.com/inventory.html",
    "elements": [
      {
        "name": "Product Name",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_name']",
        "backup_locator": ".inventory_item_name",
        "description": "The name of the product."
      },
      {
        "name": "Product Description",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_desc']",
        "backup_locator": ".inventory_item_desc",
        "description": "The description of the product."
      },
      {
        "name": "Product Price",
        "type": "text",
        "primary_locator": "//div[@class='inventory_item_price']",
        "backup_locator": ".inventory_item_price",
        "description": "The price of the product."
      },
      {
        "name": "Add to cart button",
        "type": "button",
        "primary_locator": "//button[contains(@id, 'add-to-cart')]",
        "backup_locator": "button[class*='btn_inventory']",
        "description": "Button to add the product to the shopping cart."
      },
      {
        "name": "Sorting Dropdown",
        "type": "dropdown",
        "primary_locator": "[data-test='product_sort_container']",
        "backup_locator": ".product_sort_container",
        "description": "Dropdown to sort the products by name or price."
      },
      {
        "name": "Sorting Option: Name (A to Z)",
        "type": "option",
        "primary_locator": "//option[@value='az']",
        "backup_locator": "option[value='az']",
        "description": "Option to sort products by name (A to Z)."
      },
      {
        "name": "Sorting Option: Name (Z to A)",
        "type": "option",
        "primary_locator": "//option[@value='za']",
        "backup_locator": "option[value='za']",
        "description": "Option to sort products by name (Z to A)."
      },
      {
        "name": "Sorting Option: Price (low to high)",
        "type": "option",
        "primary_locator": "//option[@value='lohi']",
        "backup_locator": "option[value='lohi']",
        "description": "Option to sort products by price (low to high)."
      },
      {
        "name": "Sorting Option: Price (high to low)",
        "type": "option",
        "primary_locator": "//option[@value='hilo']",
        "backup_locator": "option[value='hilo']",
        "description": "Option to sort products by price (high to low)."
      }
    ],
    "assertions": [
      "Page title is 'Swag Labs'",
      "Page contains the header 'Products'",
      "At least one product is displayed"
    ]
  }
}